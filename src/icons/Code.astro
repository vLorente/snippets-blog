---
interface Props {
	width?: string
	heigth?: string
	class?: string
}

const { width = "24", heigth = "24", class: className } = Astro.props
---

<svg
	class:list={[className]}
	aria-hidden="true"
	xmlns="http://www.w3.org/2000/svg"
	width={width}
	height={heigth}
	fill="none"
	viewBox="0 0 24 24"
>
	<path
		stroke="currentColor"
		stroke-linecap="round"
		stroke-linejoin="round"
		stroke-width="2"
		d="m8 8-4 4 4 4m8 0 4-4-4-4m-2-3-4 14"></path>
</svg>
