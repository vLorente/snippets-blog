---
interface Props {
	width?: string
	heigth?: string
	class?: string
}

const { width = "24", heigth = "24", class: className } = Astro.props
---

<svg
	class:list={className}
	aria-hidden="true"
	xmlns="http://www.w3.org/2000/svg"
	width={width}
	height={heigth}
	fill="none"
	viewBox="0 0 24 24"
>
	<path
		stroke="currentColor"
		stroke-linecap="round"
		stroke-width="2"
		d="M5 7h14M5 12h14M5 17h14"></path>
</svg>
